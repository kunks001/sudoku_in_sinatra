<html>
<head>
  <title>Sudoku Game</title>
  <link rel="stylesheet" type="text/css" href="/stylesheets/application.css">
</head>

<body>

  <h1>Life is too short for playing sudoku</h1>
  
  <div id="Sudoku">
    <% 3.times do |row| %>
      <div class="row">
        <% 3.times do |box| %>
          <div class="box">
            <% 3.times do |cell_row| %>
              <div class="cell_row">
                <% 3.times do |cell| %>
                  <div class="cell">
                    <input type="text" maxlength="1" autocomplete="off">
                  </div>
                <%end%>
              </div>
            <%end%>
          </div>
        <%end%>
      </div>
    <%end%>
  </div>
</body>
</html>